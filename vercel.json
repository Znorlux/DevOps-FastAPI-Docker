{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],

    "routes": [
    {"src": "/", "dest": "/main.py"},
    {"src": "/endpoint1", "dest": "/main.py"},
    {"src": "/endpoint2", "dest": "/main.py"},
    {"src": "/ruta/endpoint1", "dest": "/main.py"}
    ],

    "env": {
      "APP_MODULE": "main:app"
    }
  }
  